{
  "name": "coding-hours-forecaster",
  "version": "1.0.0",
  "description": "Coding Hours Forecaster for SNU Hackathons",
  "main": "api/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node api/index.js",
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "install-all:win": "npm install & cd client & npm install"
  },
  "keywords": ["hackathon", "coding-hours", "forecast"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  }
}


